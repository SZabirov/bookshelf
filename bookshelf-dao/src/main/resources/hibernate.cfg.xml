<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
    "-//Hibernate/Hibernate Configuration DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="connection.url">jdbc:postgresql://localhost:5432/Bookshelf</property>
    <property name="connection.driver_class">org.postgresql.Driver</property>

    <mapping class="com.fujitsu.fs.javalab.bookshelf.service.models.Author"/>
      <mapping class="com.fujitsu.fs.javalab.bookshelf.service.models.AuthorBookname"/>
      <mapping class="com.fujitsu.fs.javalab.bookshelf.service.models.Book"/>
    <mapping class="com.fujitsu.fs.javalab.bookshelf.service.models.Token"/>
    <mapping class="com.fujitsu.fs.javalab.bookshelf.service.models.Users"/>
    <mapping class="com.fujitsu.fs.javalab.bookshelf.service.models.UsersHaving"/>
    <mapping class="com.fujitsu.fs.javalab.bookshelf.service.models.UsersWish"/>
  </session-factory>
</hibernate-configuration>